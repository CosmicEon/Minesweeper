<div class="row">
    <h3>Highscore</h3>
    <table class="table table-hover">
        <thead>
            <tr>
                <th>#</th>
                <th>Player Name</th>
                <th>Score</th>
            </tr>
        </thead>
        <tbody>
            {{#each archive}}
            <tr>
                <td>{{@index}}</td>
                <td>{{archive.name}}</td>
                <td>{{archive.score}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>